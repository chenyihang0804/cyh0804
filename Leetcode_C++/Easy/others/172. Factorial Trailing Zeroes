//link: https://leetcode.com/problems/factorial-trailing-zeroes/

class Solution {
public:
    int trailingZeroes(int n) {
        long sum = 0;
        while (n) {
            sum += n/5;
            n /= 5;
        }
        return sum;
    }
};

//https://blog.csdn.net/taoyanqi8932/article/details/52129450
